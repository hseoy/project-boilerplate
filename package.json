{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:fe": "env-cmd -f .env.dev webpack serve --mode development --open",
    "dev:be": "env-cmd -f .env.dev nodemon --exec ts-node ./backend/bin/www.ts --watch backend/ -e ts",
    "dev": "npm-run-all --parallel dev:fe dev:be",
    "prestart": "env-cmd webpack --mode production",
    "start": "env-cmd ts-node ./backend/bin/www.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^6.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.1",
    "mini-css-extract-plugin": "^2.1.0",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "sass": "^1.36.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0"
  }
}
